@model IEnumerable<BlogApplication.Models.Article>

@foreach (var item in Model)
{
    
    <span class="label label-warning"> @Html.DisplayFor(modelItem=>item.Title)</span>
    <br /> 
        @Html.DisplayFor(modelItem=>item.Content)
    <br />
    @Html.ActionLink("Read", "Edit", new {id=item.Id })
    <br />
}