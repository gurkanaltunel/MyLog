@model IEnumerable<BlogApplication.Models.Categories>
@{
    Layout = null;
 }
    <ul class="nav nav-pills nav-stacked">
        <li class="active"><a href="#">Categories</a></li>
        @foreach (var item in Model)
{
            <li><a href="#" onclick="location.href='@Url.Action("ArticleCategory", "Home", new {id=item.Id })'">@Html.DisplayFor(modelItem=>item.CategoryName)</a></li>
}
    </ul>